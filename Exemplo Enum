#include<stdio.h>
//enum - enumeração

enum dias_da_semana{
	segunda,
	terca,
	quarta,
	quinta,
	sexta,
	sabado,
	domingo
};

int main (){

	enum dias_da_semana d1, d2;

	d1= quinta;
	d2 = 4;

	if(d1==d2){
		printf("O s dias sçao iguais.");
	}
	else{
		printf("Os dias não são iguais");
	}

	return 0;
}
